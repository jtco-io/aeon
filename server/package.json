{
  "name": "prion-server",
  "author": "Nicholas Romero <ncrmro@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.3.1",
    "npm": ">=5.6.0"
  },
  "version": "0.0.1",
  "scripts": {
    "start": "concurrently --kill-others \"yarn run dev:webpack\" \"yarn run dev:server\"",
    "dev:webpack": "webpack --watch --config webpack.config.js",
    "dev:server": "nodemon ./build",
    "build": "webpack --config webpack.config.js",
    "prettier": "prettier-tslint fix \"src/**/*.+(ts|tsx)\"",
    "lint": "tslint --project ./ \"src/**/*.+(ts|tsx)\""
  },
  "dependencies": {
    "@types/node": "^10.5.2",
    "apollo-datasource-rest": "^2.0.0-rc.0",
    "apollo-server": "^2.0.0-rc.0",
    "dotenv": "^6.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "ts-loader": "^4.4.2",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "graphql-cli": "2.16.3",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.7",
    "prettier-tslint": "^0.4.0",
    "ts-node": "6.1.0",
    "tsc-watch": "^1.0.22",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "2.9.2"
  }
}
