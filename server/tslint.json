{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "ter-arrow-parens": false,
    "ter-prefer-arrow-callback": false,
    "space-before-function-paren": false,
    "one-variable-per-declaration": false,
    "quotemark": false,
    "prefer-const": false,
    "no-boolean-literal-compare": false
  },
  "rulesDirectory": [
  ],
  "exclude": [
    "**/node_modules/**"
  ]
}
