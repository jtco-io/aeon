{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "no-unnecessary-initializer": false,
    "quotemark": false,
    "prefer-const": false,
    "one-variable-per-declaration": false,
    "no-parameter-reassignment": false,
    "import-name": false,
    "ter-arrow-parens": false
  },
  "rulesDirectory": [],
  "linterOptions": {
   "exclude": [
     "**/node_modules/**"
   ]
  }
}
